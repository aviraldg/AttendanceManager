<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dotinc.attendancemanager2">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SubjectsActivity" />
        <activity android:name=".WeeklySubjectsActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".NameAndCriteriaActivity" />
        <activity android:name=".ChooseAvatarActivity" />
        <activity android:name=".DetailedAnalysisActivity" />
        <activity android:name=".GoToDateActivity" />
        <activity android:name=".OverallAttendanceActivity" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".ShowNameCardActivity" />
        <activity android:name=".ExtraClassActivity" />
        <activity android:name=".FeedbackActivity" />
        <activity android:name=".HelpActivity" />
        <activity android:name=".StartScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".AboutUsActivity"></activity>
        <activity android:name=".GoToDateOldActivity" />
        <activity android:name=".PredictorActivity"></activity>

    </application>

</manifest>
